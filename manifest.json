{
	"author": "Josue Kouka",
	"manifest_version": 2,
	"default_locale": "fr",
	"name": "__MSG_appName__",
	"description": "__MSG_appDesc__",
	"short_name": "Smart Cache Cleaner",
	"version": "1.5.1",
	"homepage_url": "https://chrome.google.com/webstore/detail/smart-cache-cleaner/mjdjfdpghmnnjmlnoagnlbbmcljcmpkc",
	"icons": {
		"16": "static/img/icon.png",
		"48": "static/img/icon48.png",
		"128": "static/img/icon128.png"
	},
	"browser_action": {
		"default_title": "Cache Cleaner",
		"default_popup": "views/popup.html",
		"default_icon": "static/img/icon.png"
	},
	"background": {
		"scripts": ["controllers/background.js"],
		"persistant": true
	},
	// "options_page": "options.html",
	"permissions": [
		"tabs",
		"browsingData",
		"storage",
		"http://*/",
		"file:///*"
	],
	"offline_enabled": true
}